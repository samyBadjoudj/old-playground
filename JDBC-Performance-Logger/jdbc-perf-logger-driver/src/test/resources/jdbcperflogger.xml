<jdbc-perf-logger>
  <local-server port="8889" />
  <target-console host="localhost" port="4561" />
  <jdbc-drivers>
    <jdbc-driver>
      <prefix>jdbc:oracle: </prefix>
      <driver-class-name>oracle.jdbc.OracleDriver </driver-class-name>
    </jdbc-driver>
    <jdbc-driver>
      <prefix>jdbc:mydriver:</prefix>
      <driver-class-name>com.MyDriver</driver-class-name>
    </jdbc-driver>
  </jdbc-drivers>
</jdbc-perf-logger>
